{
  "name": "romeoz/rock-captcha",
  "description": "Captcha library",
  "keywords": [
    "captcha", "spam", "abuse"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "email": "serggalka@gmail.com",
    "issues": "https://github.com/romeOz/rock-captcha/issues",
    "source": "https://github.com/romeOz/rock-captcha"
  },
  "authors": [
    {
      "name": "Ruslan",
      "email": "serggalka@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "romeoz/rock-base": "*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.7.0",
    "romeoz/rock-session": "0.11.*",
    "gregwar/captcha" : "1.*",
    "dapphp/securimage" : "3.6.*"
  },
  "suggest": {
    "romeoz/rock-session": "required for write code to session",
    "gregwar/captcha" : "required for using GregwarCaptcha (https://github.com/Gregwar/Captcha)",
    "dapphp/securimage" : "required for using SecurimageCaptcha (https://github.com/dapphp/securimage)"
  },
  "autoload": {
    "psr-4": {
      "rock\\captcha\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.10-dev"
    }
  }
}